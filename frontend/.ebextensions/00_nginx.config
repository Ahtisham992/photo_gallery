files:
  "/etc/nginx/conf.d/00_elastic_beanstalk_proxy.conf":
    mode: "000644"
    owner: root
    group: root
    content: |
      server {
          listen 8080;
          
          location / {
              root /var/app/current;
              try_files $uri $uri/ /index.html;
          }
      }
